<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <profiles>
        <profile>
            <id>dev</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>jspresso-snapshots</id>
                    <name>Jspresso Snapshots</name>
                    <url>http://repository.jspresso.org/maven2-snapshots/</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>jspresso</id>
                    <name>Jspresso Releases</name>
                    <url>http://repository.jspresso.org/maven2/</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>jspresso-snapshots</id>
                    <name>Jspresso Snapshots</name>
                    <url>http://repository.jspresso.org/maven2-snapshots/</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>jspresso</id>
                    <name>Jspresso Releases</name>
                    <url>http://repository.jspresso.org/maven2/</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
</settings>
